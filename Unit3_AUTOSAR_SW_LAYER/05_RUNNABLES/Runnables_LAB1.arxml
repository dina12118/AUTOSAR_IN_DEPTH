<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00046.xsd">

	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>Runnables_LAB1</SHORT-NAME>

			<!-- Autosar in Depth - Runnables LAB1 -->
			<ELEMENTS>
				<!-- Interface -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_Throttle</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Throttle_CMD</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- SWC -->
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>ThrottleControl_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_SRI_ThCMD</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_Throttle</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>



					</PORTS>
					<!-- Internal Behavior -->
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>ThrottleController_Beh</SHORT-NAME>
							<RUNNABLES>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>Set_CMD_Runnable</SHORT-NAME>
									<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
									<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>
									<DATA-WRITE-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>CMD_Data</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF DEST="P-PORT-PROTOTYPE">/ThrottleControl_SWC/PP_SRI_ThCMD</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/SRI_Throttle/Throttle_CMD</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-WRITE-ACCESSS>
									<SYMBOL>Set_CMD</SYMBOL>
								</RUNNABLE-ENTITY>
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
					</INTERNAL-BEHAVIORS>
				</APPLICATION-SW-COMPONENT-TYPE>

			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>

</AUTOSAR>
