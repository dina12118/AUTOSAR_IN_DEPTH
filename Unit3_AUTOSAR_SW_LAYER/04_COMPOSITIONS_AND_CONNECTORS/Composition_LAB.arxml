<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>Composition_Lab</SHORT-NAME>
			<!-- Autosar in Depth - Composition Lab -->
			<ELEMENTS>
				<!-- Interfaces Definition -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_Light_Control</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>LightControl</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_Light_Actuate</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>LightActuateValue</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- SWCs Definition -->
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Algorithm_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_SR_LightControl</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_Light_Control</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_SR_LightActuate</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_Light_Actuate</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</APPLICATION-SW-COMPONENT-TYPE>
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LightControlStatus_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_SR_LightControl</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_Light_Control</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LightActuate_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_SR_LightActuate</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_Light_Actuate</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>

				<!-- Composition SWCs Definition -->
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SW_Composite1</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_SR_LightActuate_Composite</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_Light_Actuate</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LC_SA_SWC1</SHORT-NAME>
							<TYPE-TREF DEST="SENSOR-ACTUATOR-SW-COMPONENT-TYPE">/LightControlStatus_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LA_APP_SWC2</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/Algorithm_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>Assembly_LightControl</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/SW_Composite1/LC_SA_SWC1</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/LightControlStatus_SWC/PP_SR_LightControl</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/SW_Composite1/LA_APP_SWC2</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/Algorithm_SWC/RP_SR_LightControl</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
						<DELEGATION-SW-CONNECTOR>
							<SHORT-NAME>Delegation_LightActuate</SHORT-NAME>
							<INNER-PORT-IREF>
								<P-PORT-IN-COMPOSITION-INSTANCE-REF>
									<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/SW_Composite1/LA_APP_SWC2</CONTEXT-COMPONENT-REF>
									<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/Algorithm_SWC/PP_SR_LightActuate</TARGET-P-PORT-REF>
								</P-PORT-IN-COMPOSITION-INSTANCE-REF>
							</INNER-PORT-IREF>
							<OUTER-PORT-REF DEST="P-PORT-PROTOTYPE">/SW_Composite1/PP_SR_LightActuate_Composite</OUTER-PORT-REF>
						</DELEGATION-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>TopLevel_Composite_SWC</SHORT-NAME>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>SW_Composite1_instant</SHORT-NAME>
							<TYPE-TREF DEST="COMPOSITION-SW-COMPONENT-TYPE">/SW_Composite1</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LA_SA_SWC3</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/LightActuate_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>Assembly_LightActuate</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/TopLevel_Composite/SW_Composite1_instant</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/SW_Composite1/PP_SR_LightActuate_Composite</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/TopLevel_Composite/LA_SA_SWC3</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/LightActuate_SWC/RP_SR_LightActuate</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>
